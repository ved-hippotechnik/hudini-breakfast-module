{
  "name": "hudini-breakfast-module",
  "version": "1.0.0",
  "description": "Breakfast Module with Oracle OHIP Integration",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install"
  },
  "keywords": [
    "healthcare",
    "nutrition",
    "OHIP",
    "Oracle",
    "breakfast-tracking"
  ],
  "author": "Hudini Healthcare",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "oracledb": "^6.2.0",
    "joi": "^17.9.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.10.0",
    "axios": "^1.5.0",
    "node-cron": "^3.0.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.0",
    "@types/multer": "^1.4.7",
    "typescript": "^5.1.6",
    "ts-node-dev": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0"
  }
}
